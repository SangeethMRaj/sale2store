
<body>
        <div id="content" class="main-content">
            <div class="container">
                <div class="page-header">
                    <div class="page-title">
                        <h3>Coupon History</h3>
                      
                    </div>
                </div>
                
                <div class="row layout-spacing">
                    <div class="col-lg-12">
                        <div class="statbox widget box box-shadow">
                            <div class="widget-header">
                                <div class="row">
                                    <div class="col-xl-12 col-md-12 col-sm-12 col-12">
                                       
                                    </div>                  
                                </div>
                            </div>
                            <div class="widget-content widget-content-area">
                                <div class="table-responsive mb-4">
                                    <table id="column-filter" class="table table-bordered table-hover">
                                        <thead>
                                            <tr class="align-center">
                                                
                                                <th class="checkbox-column"> <h4>Coupon Name</h4></th>
                                                <th><h4>Discount</h4></th>
                                                <th><h4>Expiry Date</h4></th>
                                                <th><h4>Users</h4></th>
                                                <th><h4>Products</h4></th>
                                               
                                            </tr>
                                        </thead>
                                        <tbody>
                                            
                                            <tr class="align-center">
                                               
                                                <td class="checkbox-column"> <h4>{{couponHistory.[0].coupon}}</h4> </td>
                                                <td><h4>{{couponHistory.[0].discount}}</h4></td>
                                                <td><h4>{{couponHistory.[0].expdate}}</h4></td>
                                                <td>
                                                    {{#each couponHistory}}
                                                    <h4>{{inc @index}} {{this.users.Name}}</h4>
                                                    {{/each}}
                                                    
                                                    </td>
                                                    <td>
                                                    {{#each couponHistoryProducts}}
                                                    <h4>{{inc @index}} {{this.product.name}}</h4>
                                                    {{/each}}
                                                    </td>
                                            </tr>
                                       
                                        </tbody>
                                        
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>   
</body>
